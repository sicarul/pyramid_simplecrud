#!/home/sicarul/simplecrud/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'repoze.sendmail==4.1','console_scripts','qp'
__requires__ = 'repoze.sendmail==4.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('repoze.sendmail==4.1', 'console_scripts', 'qp')()
    )
